    "version": "0.2.0",
    "configurations": [
        {
            "name": "Tasks gorm storage",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            // "program": "${fileDirname}",
            "program": "${workspaceFolder}/rest/go/main.go",
            "args": [
                "-storage",
                "gorm",
                "-migrate-db",
                "-max-db-conns",
                "60"
            ],
            "showLog": false
        },
        {
            "name": "Tasks gorm-gen storage",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            // "program": "${fileDirname}",
            "program": "${workspaceFolder}/rest/go/main.go",
            "args": [
                "-storage",
                "gorm-gen",
                "-migrate-db",
                "-max-db-conns",
                "60"
            ],
            "showLog": false
        },
        {
            "name": "Tasks sql storage",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            // "program": "${fileDirname}",
            "program": "${workspaceFolder}/rest/go/main.go",
            "args": [
                "-storage",
                "sql",
                "-migrate-db",
                "-max-db-conns",
                "60"
            ],
            "showLog": false
        },
        {
            "name": "Tasks sql storage postgres-port:5433",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            // "program": "${fileDirname}",
            "program": "${workspaceFolder}/rest/go/main.go",
            "args": [
                "-storage",
                "sql",
                "-migrate-db",
                "-max-db-conns",
                "60",
                "-dsn",
                "host=localhost port=5433 user=postgres password=postgres dbname=postgres sslmode=disable client_encoding=UTF-8"
            ],
            "showLog": false
        },
        {
            "name": "Tasks pgx storage",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            // "program": "${fileDirname}",
            "program": "${workspaceFolder}/rest/go/main.go",
            "args": [
                "-storage",
                "pgx",
                "-migrate-db",
                "-max-db-conns",
                "60"
            ],
            "showLog": false
        },
        {
            "name": "Tasks grpc gateway storage",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            // "program": "${fileDirname}",
            "program": "${workspaceFolder}/rest/go/main.go",
            "args": [
                "-storage",
                "pgx",
                "-migrate-db",
                "-max-db-conns",
                "60",
                "-engine",
                "grpc-with-gateway",
                "-dsn",
                "host=localhost port=5434 user=postgres password=postgres dbname=postgres sslmode=disable client_encoding=UTF-8"
            ],
            "showLog": false
        }
    ]
}